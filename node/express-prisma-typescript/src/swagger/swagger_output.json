          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Validation Error"
                },
                "code": {
                  "type": "number",
                  "example": 400
                },
                "errors": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "message": {
                        "type": "string"
                      }
                    },
                    "required": [
                      "message"
                    ]
                  }
                }
              },
              "required": [
                "message",
                "code",
                "errors"
              ]
            }
          }
        }
      },
      "Conflict": {
        "description": "Conflict",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "Conflict"
                },
                "code": {
                  "type": "number",
                  "example": 409
                },
                "errors": {
                  "type": "object",
                  "properties": {
                    "error_code": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        }
